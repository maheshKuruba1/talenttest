package routines;

public class CategoryCodeUtils {

    public static String groupListPPX(String categoryCode) {
		if (categoryCode != null) 
		{
    		if (categoryCode.equals("IS")) 
    		{
    			return "PPMI";        			
    		}
    		else if(categoryCode.equals("NS")) 
    		{
    			return "PPMI";        			
    		}
    		else if(categoryCode.equals("M")) 
    		{
    			return "PPMI";        			
    		}
    		else 
    		{
    			return "PPHQ";        			
    		}
    	}
		// We may have data in HR NEt with no category Code throw new IllegalStateException("We must receive a categoryCode '"+categoryCode+"'");
		return "PPHQ";
    }
}
